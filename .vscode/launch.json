    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "flutter_template Prod",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "${workspaceFolder}/lib/flavors/app_prod.dart",
             "args": ["--flavor","prod", "--target" , "lib/flavors/app_prod.dart"]
        },
        {
            "name": "flutter_template QA",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "program": "lib/flavors/app_qa.dart",
            "args": [
                "--flavor","dev",
                "--target" , "lib/flavors/app_qa.dart",
                "--dart-define-from-file", "api-keys.dev.json"
            ]
        }
    ,
